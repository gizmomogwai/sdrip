doctype html
html
  head
    title& welcome
    include jquery
    include bootstrap
    link(rel="icon", type="image/png", href="/favicon.png")
  body
    h1
      a(href="/") back to index

    h1 Version
      table
        th
          tr
            td package
            td version
            td license
        - foreach (p; packages)
          tr
            td {p.name}
            td {p.semVer}
            td {p.license}
  include footer
